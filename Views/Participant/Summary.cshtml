@model RMDProcessingApp.Models.Participant

@{
    ViewData["Title"] = "Participant Summary";
}

<h2>Participant Summary</h2>

<table class="table">
    <tr>
        <td><strong>Participant ID</strong></td>
        <td>@Model.ParticipantId</td>
    </tr>
    <tr>
        <td><strong>Name</strong></td>
        <td>@Model.FullName</td>
    </tr>
    <tr>
        <td><strong>Plan ID</strong></td>
        <td>@Model.PlanId</td>
    </tr>
    <tr>
        <td><strong>Age</strong></td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td><strong>RMD Status</strong></td>
        <td>@Model.RmdStatus</td>
    </tr>
</table>
